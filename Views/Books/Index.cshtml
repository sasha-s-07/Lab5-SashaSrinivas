@model IEnumerable<Lab5_SashaSrinivas.Models.Book>
@{
    ViewData["Title"] = "Books";
}

<h1>Books</h1>
<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>Id</th>
            <th>Title</th>
            <th>First Name</th>
            <th>Middle Name</th>
            <th>Last Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@Html.DisplayFor(m => item.id)</td>
                <td>@Html.DisplayFor(m => item.Title)</td>
                <td>@Html.DisplayFor(m => item.FirstName)</td>
                <td>@Html.DisplayFor(m => item.MiddleName)</td>
                <td>@Html.DisplayFor(m => item.LastName)</td>
            </tr>
        }
    </tbody>


</table>
<p>@Html.ActionLink("Create a book", "Create", "Books")</p>


